class Solution:
    def getCommon(self, nums1: List[int], nums2: List[int]) -> int:
        nums2 = set(nums2)
        for i in nums1:
            if i in nums2:
                return i
        return -1

# type convert the nums2 to a hash set to achieve the O(1) complexity within the looping
# Overall Time Complexity: 0(n)
